<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER"
      xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.gymbro.window.fxml.FavoritesMenuFxmlController"
      prefHeight="960.0" prefWidth="840.0"
      spacing="5"
>

    <Button id="start" onAction="#favoritesMap" text="Show favorites"/>
    <Button id="save" onAction="#saveFavorites" text="Save favorites"/>
    <Button id="back" onAction="#backToMainMenu" text="Back"/>


</VBox>
